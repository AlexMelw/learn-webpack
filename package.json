{
  "name": "learn-webpack",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "up-major": "git restore --staged . && npm version major --message \"UpRev %s\" --force",
    "up-minor": "git restore --staged . && npm version minor --message \"UpRev %s\" --force",
    "up-patch": "git restore --staged . && npm version patch --message \"UpRev %s\" --force",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack-dev-server --mode development --config webpack.dev.config.js",
    "build-dev": "webpack --config webpack.dev.config.js",
    "build-prod": "webpack --config webpack.prod.config.js",
    "build-public": "webpack --config webpack.public.config.js --mode production",
    "build-admin": "webpack --config webpack.admin.config.js --mode production",
    "build-frontend": "webpack --config webpack.frontend.config.js --mode production",
    "build-server": "webpack --config webpack.server.config.js --mode production",
    "build-admin-n-public": "npm run build-public && npm run build-admin",
    "build-frontend-n-server": "npm run build-frontend && npm run build-server",
    "build": "npm run build-frontend-n-server && npm run build-admin-n-public",
    "build-experimental-both-A": "webpack --config webpack.config.js --mode development",
    "build-experimental-both-B": "npm run build-experimental-frontend-only && npm run build-experimental-server-only",
    "build-experimental-frontend-only": "webpack --config webpack.config.js --mode development --config-name frontend",
    "build-experimental-server-only": "webpack --config webpack.config.js --mode development --config-name server",
    "cli": "node ./dist/main.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.7.4",
    "html-webpack-plugin": "^5.5.1",
    "sass": "^1.62.1",
    "sass-loader": "^13.2.2",
    "style-loader": "^3.3.3",
    "webpack": "^5.80.0",
    "webpack-cli": "^5.0.2",
    "webpack-dev-server": "^4.15.0",
    "webpack-merge": "^5.9.0",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
